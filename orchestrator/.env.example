# Supabase connection (Settings > API)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=eyJ...      # service_role key (required â€” bypasses RLS)

# Orchestrator identity (unique name for this PC)
ITACHI_ORCHESTRATOR_ID=windows-pc

# Concurrency
ITACHI_MAX_CONCURRENT=2

# Workspace directory for cloned repos / worktrees
ITACHI_WORKSPACE_DIR=C:\Users\newma\itachi-workspaces

# Task timeout in milliseconds (default: 10 minutes)
ITACHI_TASK_TIMEOUT_MS=600000

# Default Claude model
ITACHI_DEFAULT_MODEL=sonnet

# Default budget per task in USD
ITACHI_DEFAULT_BUDGET=5.00

# Poll interval in milliseconds (default: 5 seconds)
ITACHI_POLL_INTERVAL_MS=5000

# Project paths JSON map (for worktree mode - local repos)
# Format: {"project-name": "/path/to/repo"}
ITACHI_PROJECT_PATHS={}

# Railway API base URL (for task notifications)
ITACHI_API_URL=https://itachisbrainserver.online
